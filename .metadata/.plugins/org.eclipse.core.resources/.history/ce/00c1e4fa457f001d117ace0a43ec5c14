package com.domain;

import java.util.Date;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

import lombok.Data;
@Data // lombok : setter, getter, tostring ...

//*********
@Entity
@Table(name = "EMP_A")
public class EmpVO2 {
	
	@Id
	private int empno;
	
	private String ename;
	private String job;
	private int mgr;
	private Date hiredate;
	
	private int sal;
	private int comm;
	private int deptno;	

}
